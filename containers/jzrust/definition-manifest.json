{
	"definitionVersion": "0.100.0",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"arm32v7/rust:${VERSION}"
		]
	},
	"dependencies": {
		"image": "arm32v7/rust",
		"imageLink": "https://hub.docker.com/_/rust/",
		"git": {
			"Oh My Bash!": "/root/.oh-my-bash",
			"Oh My Zsh!": "/root/.oh-my-zsh"
		},
		"cargo": {
			"rls": "rls --version",
			"rustfmt": "rustfmt --version",
			"rust-analysis": null,
			"rust-src": null,
			"clippy": null
		}
	}
}
