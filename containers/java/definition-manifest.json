{
	"variants": [ "11-ubuntu", "17-ubuntu" ],
	"definitionVersion": "0.204.0",
	"build": {
		"latest": "17-ubuntu",
		"rootDistro": "ubuntu",
		"variantBuildArgs": {
			"17-ubuntu": {
				"TARGET_JAVA_VERSION": "17",
				"BASE_IMAGE_VERSION_CODENAME": "ubuntu"
			},
			"11-ubuntu": {
				"TARGET_JAVA_VERSION": "11",
				"BASE_IMAGE_VERSION_CODENAME": "ubuntu"
			}
		},
		"architectures": {
			"17-ubuntu": ["linux/amd64", "linux/arm64"],
			"11-ubuntu": ["linux/amd64", "linux/arm64"]
		},
		"tags": [
			"java:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"17-ubuntu": [ 
				"java:${VERSION}-17",
				"java:${VERSION}-17-ubuntu",
				"java:${VERSION}-ubuntu"
			],
			"11-ubuntu": [ 
				"java:${VERSION}-11",
				"java:${VERSION}-11-ubuntu" 
			]
		}
	},
	"dependencies": {
		"image": "mcr.microsoft.com/openjdk/jdk:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/openjdk",
		"apt": [{
			"cgIgnore": false,
			"name": "yarn"
		}],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"other": {
			"SDKMAN!": null
		},
		"languages": {
			"Java": {
				"cgIgnore": true,
				"path": "/usr/local/"
			}
		}
	}
}
